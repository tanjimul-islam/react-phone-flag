{
  "name": "react-phone-flag",
  "version": "1.0.0",
  "type": "module",
  "description": "A React component for selecting country codes with flags for phone numbers",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "phone",
    "country-code",
    "flag",
    "international",
    "phone-number",
    "country-selector"
